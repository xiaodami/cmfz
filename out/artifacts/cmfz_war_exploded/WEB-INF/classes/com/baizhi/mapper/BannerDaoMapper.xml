  <update id="update">
  	update Banner set title=#{title},img_path=#{img_path},description=#{description},status=#{status},createtime=#{createTime} where id=#{id}
  </update>
  <delete id="delete">
  	delete from banner where id in 
  	<foreach collection="array" open="(" item="t" separator="," close=")">
       #{t}
   </foreach>
  </delete>
  <insert id="insert">
  	insert into Banner values(#{id},#{title},#{img_path},#{description},#{status},#{createTime},"","","","","")
  </insert>
